{
  "id": null,
  "title": "Energy Storage Monitoring",
  "tags": ["energy-storage", "battery", "soc", "mvp"],
  "style": "dark",
  "timezone": "utc",
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": [
      {
        "name": "battery_id",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> filter(fn: (r) => r.device_type == \"energy_storage\") |> group(columns: [\"device_id\"]) |> distinct(column: \"device_id\")",
        "datasource": "InfluxDB 2.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "soc_range",
        "type": "custom",
        "query": "<20%, 20-40%, 40-60%, 60-80%, 80-100%",
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Stored Energy",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"stored_energy\")\n  |> sum()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "red", "value": 10},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 100}
            ]
          },
          "unit": "kwatth"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Average State of Charge",
      "type": "gauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"state_of_charge\")\n  |> mean()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "red", "value": 20},
              {"color": "yellow", "value": 40},
              {"color": "green", "value": 80},
              {"color": "yellow", "value": 90},
              {"color": "red", "value": 100}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "Battery Temperature",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"temperature\")\n  |> mean()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "blue", "value": null},
              {"color": "blue", "value": 10},
              {"color": "green", "value": 35},
              {"color": "yellow", "value": 45},
              {"color": "red", "value": 60}
            ]
          },
          "unit": "celsius"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "State of Charge Trends",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -24h)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"state_of_charge\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)\n  |> group(columns: [\"device_id\"])",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
    },
    {
      "id": 5,
      "title": "Voltage and Current Trends",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -24h)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"voltage\" or r._field == \"current\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "voltage"
            },
            "properties": [
              {
                "id": "unit",
                "value": "volt"
              },
              {
                "id": "custom.axisPlacement",
                "value": "left"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "current"
            },
            "properties": [
              {
                "id": "unit",
                "value": "amp"
              },
              {
                "id": "custom.axisPlacement",
                "value": "right"
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 6,
      "title": "Battery Health",
      "type": "gauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"health\")\n  |> mean()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "red", "value": 60},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 100}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    },
    {
      "id": 7,
      "title": "Charge/Discharge Cycles",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -24h)\n  |> filter(fn: (r) => r.device_type == \"energy_storage\")\n  |> filter(fn: (r) => r._field == \"charge_cycles\" or r._field == \"discharge_cycles\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
