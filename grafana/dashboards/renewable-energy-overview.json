{
  "id": null,
  "title": "Renewable Energy System Overview",
  "tags": ["renewable-energy", "overview", "mvp"],
  "style": "dark",
  "timezone": "utc",
  "refresh": "30s",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": [
      {
        "name": "device_type",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> group(columns: [\"device_type\"]) |> distinct(column: \"device_type\")",
        "datasource": "InfluxDB 3.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "location",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> group(columns: [\"location\"]) |> distinct(column: \"location\")",
        "datasource": "InfluxDB 3.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Power Generation",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._field == \"power_output\")\n  |> sum()",
          "datasource": "InfluxDB 3.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0}
            ]
          },
          "unit": "watt"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Active Devices Count",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._field == \"status\")\n  |> filter(fn: (r) => r._value == \"operational\")\n  |> group()\n  |> count()",
          "datasource": "InfluxDB 3.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "System Efficiency",
      "type": "gauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._field == \"efficiency\")\n  |> mean()",
          "datasource": "InfluxDB 3.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 60},
              {"color": "green", "value": 80}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "Power Generation by Device Type",
      "type": "piechart",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._field == \"power_output\")\n  |> group(columns: [\"device_type\"])\n  |> sum()",
          "datasource": "InfluxDB 3.x"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "pieType": "pie",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
    },
    {
      "id": 5,
      "title": "Power Generation Trends",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -24h)\n  |> filter(fn: (r) => r._field == \"power_output\")\n  |> aggregateWindow(every: 5m, fn: mean, createEmpty: false)\n  |> group(columns: [\"device_type\"])",
          "datasource": "InfluxDB 3.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "never",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "unit": "watt"
        }
      },
      "options": {
        "legend": {
          "displayMode": "visible",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
    },
    {
      "id": 6,
      "title": "Device Status Summary",
      "type": "table",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._field == \"status\" or r._field == \"power_output\")\n  |> pivot(rowKey:[\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> group(columns: [\"device_id\", \"device_type\"])\n  |> last()",
          "datasource": "InfluxDB 3.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": false,
            "displayName": "Device ID"
          }
        ]
      },
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__value": true,
              "__field": true
            },
            "indexByName": {},
            "renameByName": {
              "device_id": "Device ID",
              "device_type": "Device Type",
              "status": "Status",
              "power_output": "Power Output (W)"
            }
          }
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
    }
  ]
} 