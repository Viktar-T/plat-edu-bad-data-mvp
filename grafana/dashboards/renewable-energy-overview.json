{
  "id": null,
  "title": "Renewable Energy Overview",
  "tags": ["overview", "renewable", "energy", "mvp"],
  "style": "dark",
  "timezone": "utc",
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": [
      {
        "name": "device_type",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> group(columns: [\"_measurement\"]) |> distinct(column: \"_measurement\")",
        "datasource": "InfluxDB 2.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "location",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> group(columns: [\"location\"]) |> distinct(column: \"location\")",
        "datasource": "InfluxDB 2.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Power Generation",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._field == \"power_output\" or r._field == \"heat_output\" or r._field == \"gas_flow_rate\")\n  |> sum()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 2000},
              {"color": "red", "value": 5000}
            ]
          },
          "unit": "watt"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "System Efficiency",
      "type": "gauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._field == \"efficiency\")\n  |> mean()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "red", "value": 60},
              {"color": "yellow", "value": 80},
              {"color": "green", "value": 100}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "Active Devices",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> group(columns: [\"device_id\"])\n  |> distinct(column: \"device_id\")\n  |> count()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 20}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "Power Generation (6h)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -6h)\n  |> filter(fn: (r) => r._field == \"power_output\" or r._field == \"heat_output\")\n  |> group(columns: [\"_measurement\"])",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "watt"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 5,
      "title": "Efficiency Trends (6h)",
      "type": "timeseries",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -6h)\n  |> filter(fn: (r) => r._field == \"efficiency\")\n  |> group(columns: [\"_measurement\"])",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "drawStyle": "line",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "calcs": [
            "lastNotNull"
          ],
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    }
  ]
} 
