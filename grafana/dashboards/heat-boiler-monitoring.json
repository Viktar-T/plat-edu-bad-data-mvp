{
  "id": null,
  "title": "Heat Boiler Monitoring",
  "tags": ["heat-boiler", "thermal", "efficiency", "mvp"],
  "style": "dark",
  "timezone": "utc",
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
  },
  "templating": {
    "list": [
      {
        "name": "boiler_id",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> filter(fn: (r) => r._measurement == \"heat_boiler_data\") |> group(columns: [\"device_id\"]) |> distinct(column: \"device_id\")",
        "datasource": "InfluxDB 2.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      },
      {
        "name": "location",
        "type": "query",
        "query": "from(bucket: \"renewable_energy\") |> range(start: -1h) |> filter(fn: (r) => r._measurement == \"heat_boiler_data\") |> group(columns: [\"location\"]) |> distinct(column: \"location\")",
        "datasource": "InfluxDB 2.x",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "title": "Total Heat Output",
      "type": "stat",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -1h)\n  |> filter(fn: (r) => r._measurement == \"heat_boiler_data\")\n  |> filter(fn: (r) => r._field == \"heat_output\")\n  |> sum()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "green", "value": 0},
              {"color": "yellow", "value": 500},
              {"color": "red", "value": 1000}
            ]
          },
          "unit": "kw"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Boiler Efficiency",
      "type": "gauge",
      "targets": [
        {
          "refId": "A",
          "query": "from(bucket: \"renewable_energy\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r._measurement == \"heat_boiler_data\")\n  |> filter(fn: (r) => r._field == \"efficiency\")\n  |> mean()",
          "datasource": "InfluxDB 2.x"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "red", "value": 70},
              {"color": "yellow", "value": 85},
              {"color": "green", "value": 100}
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
